{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "service-calls-agent-api": {
      "type": "application",
      "root": "src/apps/service-calls-agent-api",
      "entryFile": "main",
      "sourceRoot": "src/apps/service-calls-agent-api"
    },
    "requests-agent-worker": {
      "type": "application",
      "root": "src/apps/requests-agent-worker",
      "entryFile": "main",
      "sourceRoot": "src/apps/requests-agent-worker"
    }
  },
  "defaultProject": "service-calls-agent-api"
}
