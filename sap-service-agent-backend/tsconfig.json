{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@sap-service-agent/service-calls/*": ["src/libs/service-calls/*"],
      "@sap-service-agent/service-calls": ["src/libs/service-calls"],
      "@sap-service-agent/shared/*": ["src/libs/shared/*"],
      "@sap-service-agent/shared": ["src/libs/shared"],
      "@sap-service-agent/auth/*": ["src/libs/auth/*"],
      "@sap-service-agent/auth": ["src/libs/auth"]
    }
  }
}
